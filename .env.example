# Environment variables for Ocean Protocol Algorithm
# Copy this file to .env and modify values as needed

# Configuration file path (defaults to /config.yaml in Docker, auto-detects in tests)
# CONFIG_PATH=/config.yaml

# Algorithm settings
ALGORITHM_NAME=age_average
ALGORITHM_VERSION=1.0.0
ALGORITHM_DESCRIPTION=Calculate age statistics from user data

# Data processing settings
MAX_FILE_SIZE_MB=100

# Logging settings
LOG_LEVEL=INFO

# Performance settings
BATCH_SIZE=1000
TIMEOUT_SECONDS=300

CONFIG_PATH=/config.yaml